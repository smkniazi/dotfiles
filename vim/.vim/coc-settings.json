{
  "go.goplsPath": "gopls",
  "go.goplsUseDaemon": false,
  "go.goplsArgs": ["-logfile=/tmp/gopls-coc.log", "-rpc.trace"],
  "go.trace.server": "verbose",
  "go.goplsOptions": {},
  "inlayHint.enable": false,
  "notification.border": true,
  "diagnostic.enable": true,
  "diagnostic.errorSign": "▶",
  "diagnostic.hintSign": "▷",
  "diagnostic.infoSign": "▷",
  "diagnostic.warningSign": "▷",
  "list.maxPreviewHeight": 20,
  "clangd.enabled": true,
  "clangd.path": "clangd",
  "clangd.arguments": [],
  "sh.enable": true,
  "yaml.schemas": {
    "kubernetes": "/*.yaml"
  },
  "python.pythonPath": "/home/salman/code/hops/arrow-dev/arrow/python/mypython",
  "workspace.ignoredFolders": [
    "$HOME",
    "$HOME/.cargo/**",
    "$HOME/.rustup/**"
  ],
  "rust-analyzer.enable": true,
  "rust-analyzer.cargo.features": "all",
  "java.configuration.runtimes": [
    {
      "name": "Java",
      "path": "~/usr/bin/jdk"
    }
  ],
  "java.jdt.ls.vmargs": "-noverify -Xmx6G -XX:+UseG1GC -XX:+UseStringDeduplication"
}
