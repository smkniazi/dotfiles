#!/bin/sh
function log(){
	message=$1
	echo -e    $1
	echo -e    $1 >> /tmp/mykeyboard.log
}
log "Uapp"
eval "xdotool windowactivate key Up"
exit 0
#sleep 0.50
xvkbd -window 0x02a00041 -text "\{+Up}\{-Up}"
log "Uapp"
exit 0
wmctrl -a firefox
xte << EOF
key Up
key Up
key Up
key Up
key Up
key Up
key Up
EOF


