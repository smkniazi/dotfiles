#!/bin/bash
mvn $@ install -Dmaven.test.skip=true -f ~/code/hops/hops-metadata-dal/pom.xml

mvn $@ install -Dmaven.test.skip=true -f ~/code/hops/hops-metadata-dal-impl-ndb/pom.xml

mvn $@ install -Dmaven.test.skip=true generate-sources -Pndb -f ~/code/hops/hops/pom.xml

#temporarily change pwd
cd ~/code/hops/hops && mvn test-compile

