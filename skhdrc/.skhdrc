#skhd --restart-service
cmd + shift - f :open -a firefox; osascript -e 'tell application "firefox" to activate' 
cmd + shift - v :osascript -e 'tell application "/opt/homebrew/bin/mpv" to activate' 
cmd + shift - c :open -a '/Applications/Google Chrome.app'; osascript -e 'tell application "chrome" to activate'
cmd + shift - i :${HOME}/.local/bin/start-idea
cmd + shift - t :open -a kitty; osascript -e 'tell application "kitty" to activate' 
cmd + shift - k :open -a slack; osascript -e 'tell application "slack" to activate'
cmd + shift - g :open -a chatgpt; osascript -e 'tell application "chatgpt" to activate'
ctrl + cmd - d  :osascript /Users/salman/.local/bin/hide-all-macos-windows
ctrl + cmd - g  :osascript /Users/salman/.local/bin/mac-move-windows-to-main-display
cmd + shift - s  :m volume up
alt + cmd + shift - s  :m volume down 
cmd + shift - b  :m display down
alt + cmd + shift - b  :m display down 
cmd -m :open -a "Mission Control"
